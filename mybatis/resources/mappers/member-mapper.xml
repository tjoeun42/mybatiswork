<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="memberMapper">
  <select id="checkId" resultType="_int">
    select count(*) from member where user_id = #{userId}
  </select>
  <insert id="insertMember" parameterType="com.mybatis.member.dto.Member">
  	insert into member 
  	values(#{userId}, #{userPwd},#{userName},#{email},#{birthday},#{gender},#{phone},#{address},default,default,default)
  </insert>
</mapper>